package com.xunyun.infanteduplatform.domain;

import java.util.Date;

/**
 * Created by Administrator on 2015/11/30.
 * 学校信息实体类
 */
public class SchoolInfo {
    private Long SchoolId;	/**数据编码	唯一编号，自增长	NUMBER*/
    private String SchoolnCode;	/**学校组织机构代码	"学校（班级，部门的上级主管部门）的唯一标识 。
    序号在行政区码和学校类型下递增"	VARCHAR2*/
    private Integer SchoolStyle;	/**学校类型	"按位存储,多学校类型组合存储
    第0位（1）：幼儿园
    第1位（2）：小学
    第2位（4）：初中
    第3位（8）：高中
    第4位（16）：特教
    第5位（32）：中等职业学校（中专、技校、职高）
    第6位（64）：高等职业学校（高职、高专）
    第7位（128）：大学（包括研究生、本科、大专）"	INTEGER*/
    private String SchoolName;	/**学校名称	指学校的标准名称，与公章一致	VARCHAR2*/
    private String ShortName;	/**学校简称		VARCHAR2*/
    private String SchoolEnglishName;	/**英文名称	指学校的英文名称	VARCHAR2*/
    private String SchoolAddress;	/**学校地址	指包括省（自治区、直辖市）／地（市、区）／县（市、旗）／乡（镇）／街（村）的详细地址	VARCHAR2*/
    private String DutyPerson;	/**联系人	联系人的姓名	VARCHAR2*/
    private Date EstablishmentDate;	/**建校年月	指最初建校年月	DATE*/
    private String SchoolHostCode;	/**学校办别码	参见DM—XXBB《学校办别代码》	VARCHAR2*/
    private String SchoolTypeCode;	/**学校类别码	参见JS—XXLB《学校类别代码》	VARCHAR2*/
    private String LocationTypeCode;	/**所在地区类别码	参见DM—SZDLB《所在地区类别代码》	VARCHAR2*/
    private String LocationEconCode;	/**所在地经济属性码	参见DM—SZDJJSX《所在地经济属性代码》	VARCHAR2*/
    private String LocationFolkCode;	/**所在地民族属性	DM—MZZZX《民族自治县状况代码》	VARCHAR2*/
    private String FirstTeachLanguage;	/**主教学语言码	参见GB／T4881《中国语种代码》	VARCHAR2*/
    private String SecondTeachLanguage;	/**辅教学语言码	参见GB／T4881《中国语种代码》	VARCHAR2*/
    private String PostalCode;	/**邮政编码	指学校所在地的邮政编码	VARCHAR2*/
    private String Telephone;	/**联系电话	学校联系电话	VARCHAR2*/
    private String Fax;	/**传真电话	学校传真	VARCHAR2*/
    private String Email;	/**电子信箱	学校联系人的电子邮件信箱地址	VARCHAR2*/
    private String HomePageUrl;	/**主页地址	在互联网上的主页地址	VARCHAR2*/
    private String SchoolBiographicalNotes;	/**学校简介		VARCHAR2*/
    private String IPAddress;	/**IP地址	在互联网上的IP地址	VARCHAR2*/
    private Integer DeleteFlg;	/**删除标记	0.未删除 1.删除	INTEGER*/
    private Date CreationTime;	/**创建时间	创建时间	DATE*/
    private String CreatedBy;	/**创建人	创建人	VARCHAR2*/
    private Date LastUpdateTime;	/**最终修改时间	最终修改时间	DATE*/
    private String LastUpdatedBy;	/**最终修改人	最终修改人	VARCHAR2*/

    public Long getSchoolId() {
        return SchoolId;
    }

    public void setSchoolId(Long schoolId) {
        SchoolId = schoolId;
    }

    public String getSchoolnCode() {
        return SchoolnCode;
    }

    public void setSchoolnCode(String schoolnCode) {
        SchoolnCode = schoolnCode;
    }

    public Integer getSchoolStyle() {
        return SchoolStyle;
    }

    public void setSchoolStyle(Integer schoolStyle) {
        SchoolStyle = schoolStyle;
    }

    public String getSchoolName() {
        return SchoolName;
    }

    public void setSchoolName(String schoolName) {
        SchoolName = schoolName;
    }

    public String getShortName() {
        return ShortName;
    }

    public void setShortName(String shortName) {
        ShortName = shortName;
    }

    public String getSchoolEnglishName() {
        return SchoolEnglishName;
    }

    public void setSchoolEnglishName(String schoolEnglishName) {
        SchoolEnglishName = schoolEnglishName;
    }

    public String getSchoolAddress() {
        return SchoolAddress;
    }

    public void setSchoolAddress(String schoolAddress) {
        SchoolAddress = schoolAddress;
    }

    public String getDutyPerson() {
        return DutyPerson;
    }

    public void setDutyPerson(String dutyPerson) {
        DutyPerson = dutyPerson;
    }

    public Date getEstablishmentDate() {
        return EstablishmentDate;
    }

    public void setEstablishmentDate(Date establishmentDate) {
        EstablishmentDate = establishmentDate;
    }

    public String getSchoolHostCode() {
        return SchoolHostCode;
    }

    public void setSchoolHostCode(String schoolHostCode) {
        SchoolHostCode = schoolHostCode;
    }

    public String getSchoolTypeCode() {
        return SchoolTypeCode;
    }

    public void setSchoolTypeCode(String schoolTypeCode) {
        SchoolTypeCode = schoolTypeCode;
    }

    public String getLocationTypeCode() {
        return LocationTypeCode;
    }

    public void setLocationTypeCode(String locationTypeCode) {
        LocationTypeCode = locationTypeCode;
    }

    public String getLocationEconCode() {
        return LocationEconCode;
    }

    public void setLocationEconCode(String locationEconCode) {
        LocationEconCode = locationEconCode;
    }

    public String getLocationFolkCode() {
        return LocationFolkCode;
    }

    public void setLocationFolkCode(String locationFolkCode) {
        LocationFolkCode = locationFolkCode;
    }

    public String getFirstTeachLanguage() {
        return FirstTeachLanguage;
    }

    public void setFirstTeachLanguage(String firstTeachLanguage) {
        FirstTeachLanguage = firstTeachLanguage;
    }

    public String getSecondTeachLanguage() {
        return SecondTeachLanguage;
    }

    public void setSecondTeachLanguage(String secondTeachLanguage) {
        SecondTeachLanguage = secondTeachLanguage;
    }

    public String getPostalCode() {
        return PostalCode;
    }

    public void setPostalCode(String postalCode) {
        PostalCode = postalCode;
    }

    public String getTelephone() {
        return Telephone;
    }

    public void setTelephone(String telephone) {
        Telephone = telephone;
    }

    public String getFax() {
        return Fax;
    }

    public void setFax(String fax) {
        Fax = fax;
    }

    public String getEmail() {
        return Email;
    }

    public void setEmail(String email) {
        Email = email;
    }

    public String getHomePageUrl() {
        return HomePageUrl;
    }

    public void setHomePageUrl(String homePageUrl) {
        HomePageUrl = homePageUrl;
    }

    public String getSchoolBiographicalNotes() {
        return SchoolBiographicalNotes;
    }

    public void setSchoolBiographicalNotes(String schoolBiographicalNotes) {
        SchoolBiographicalNotes = schoolBiographicalNotes;
    }

    public String getIPAddress() {
        return IPAddress;
    }

    public void setIPAddress(String IPAddress) {
        this.IPAddress = IPAddress;
    }

    public Integer getDeleteFlg() {
        return DeleteFlg;
    }

    public void setDeleteFlg(Integer deleteFlg) {
        DeleteFlg = deleteFlg;
    }

    public Date getCreationTime() {
        return CreationTime;
    }

    public void setCreationTime(Date creationTime) {
        CreationTime = creationTime;
    }

    public String getCreatedBy() {
        return CreatedBy;
    }

    public void setCreatedBy(String createdBy) {
        CreatedBy = createdBy;
    }

    public Date getLastUpdateTime() {
        return LastUpdateTime;
    }

    public void setLastUpdateTime(Date lastUpdateTime) {
        LastUpdateTime = lastUpdateTime;
    }

    public String getLastUpdatedBy() {
        return LastUpdatedBy;
    }

    public void setLastUpdatedBy(String lastUpdatedBy) {
        LastUpdatedBy = lastUpdatedBy;
    }
}

package com.xunyun.infanteduplatform.domain;

import java.util.Date;

/**
 * 第三方登录实体类
 * Created by Pooh on 2015/12/30.
 */
public class AccountBinding {
    public Integer userId;//用户Id
    public String appCode;//第三方系统标识
    public String account;//第三方系统用户标识
    public Date bindingTime;// 帐号绑定时间
    public Integer appType;//应用类型 0：外部应用，1：平台接入应用
    public String attributeKey1;//扩展字段Key1
    public String attributeValue1;//扩展字段Value1
    public String attributeKey2;//扩展字段Key2
    public String attributeValu2;//扩展字段Value2
    public String attributeKey3;//扩展字段Key3
    public String attributeValue3;// 扩展字段Value3

    /**
     * getter & setter
     */
    public Integer getUserId() {
        return userId;
    }

    public void setUserId(Integer userId) {
        this.userId = userId;
    }

    public String getAppCode() {
        return appCode;
    }

    public void setAppCode(String appCode) {
        this.appCode = appCode;
    }

    public String getAccount() {
        return account;
    }

    public void setAccount(String account) {
        this.account = account;
    }

    public Date getBindingTime() {
        return bindingTime;
    }

    public void setBindingTime(Date bindingTime) {
        this.bindingTime = bindingTime;
    }

    public Integer getAppType() {
        return appType;
    }

    public void setAppType(Integer appType) {
        this.appType = appType;
    }

    public String getAttributeKey1() {
        return attributeKey1;
    }

    public void setAttributeKey1(String attributeKey1) {
        this.attributeKey1 = attributeKey1;
    }

    public String getAttributeValue1() {
        return attributeValue1;
    }

    public void setAttributeValue1(String attributeValue1) {
        this.attributeValue1 = attributeValue1;
    }

    public String getAttributeKey2() {
        return attributeKey2;
    }

    public void setAttributeKey2(String attributeKey2) {
        this.attributeKey2 = attributeKey2;
    }

    public String getAttributeValu2() {
        return attributeValu2;
    }

    public void setAttributeValu2(String attributeValu2) {
        this.attributeValu2 = attributeValu2;
    }

    public String getAttributeKey3() {
        return attributeKey3;
    }

    public void setAttributeKey3(String attributeKey3) {
        this.attributeKey3 = attributeKey3;
    }

    public String getAttributeValue3() {
        return attributeValue3;
    }

    public void setAttributeValue3(String attributeValue3) {
        this.attributeValue3 = attributeValue3;
    }
}
